- keywords("#{@keyword},#{@keyword}优惠,#{@keyword}优惠券,#{@keyword}折扣,特价#{@keyword}")
- path(@path)
- h1(@keyword)

.wrapper
  = render partial: "m_diyquan/header", locals: {need_sort: true, need_ads: false}
  = render partial: "m_diyquan/page_js", locals: {path: '/getSearchCouponList', pinyin: '', keyword: @keyword, pagesize: 20, sort_type: @sort_type}
  = render partial: "m_diyquan/tb_shuangpai_list", locals: {coupons: @coupons, zhekous: @zhekous}
